for(i=0; i<4; i++){
    var a = Math.abs(5-i)
    blank1 = " ".repeat((a*2-1)/2)
    star1 = "*".repeat(i*2+1)
    console.log(blank1 + star1)
}

for(i=0; i<5; i++){
    var a1 = Math.abs(i);
    var a2 = Math.abs(5-i)
    star2 = "*".repeat(a2*2-1)
    blank2 = " ".repeat((a1*2+1)/2)
    console.log(blank2 + star2)
}
 

